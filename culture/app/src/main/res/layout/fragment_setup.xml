<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ma"
    tools:context=".home.SetupFragment">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dip"
            android:orientation="vertical" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:gravity="center_vertical"
                android:text="@string/a5"
                android:textColor="#DC413018"
                android:textSize="25sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/nor_concor"
                android:orientation="vertical" >



                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="2dip"
                    android:background="@drawable/line" />
                <RelativeLayout
                    android:id="@+id/ap"
                    android:layout_width="match_parent"
                    android:layout_height="40dip"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10sp"
                        android:layout_marginTop="5dp"
                        android:src="@drawable/c3"/>
                    <TextView
                        android:id="@+id/app_way"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="50dip"
                        android:gravity="center_vertical"
                        android:text="@string/a2"
                        android:textColor="#DC413018"
                        android:textSize="15sp" />

                    <ImageView
                        android:layout_width="20dip"
                        android:layout_height="20dip"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="5dip"
                        android:background="@drawable/back1" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/wifi"
                    android:layout_width="match_parent"
                    android:layout_height="40dip"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10sp"
                        android:layout_marginTop="5dp"
                        android:src="@drawable/c2"/>
                    <TextView
                        android:id="@+id/nearwifi"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="50dip"
                        android:gravity="center_vertical"
                        android:text="@string/a1"
                        android:textColor="#DC413018"
                        android:textSize="15sp" />

                    <ImageView
                        android:layout_width="20dip"
                        android:layout_height="20dip"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="5dip"
                        android:background="@drawable/back1" />
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dip"
            android:layout_marginTop="40dp"
            android:background="@drawable/nor_concor"
            android:orientation="vertical" >



            <ImageView
                android:layout_width="match_parent"
                android:layout_height="2dip"
                android:background="@drawable/line" />
            <RelativeLayout
                android:id="@+id/off_web"
                android:layout_width="match_parent"
                android:layout_height="40dip"
                android:orientation="horizontal" >
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="10sp"
                    android:layout_marginTop="5dp"
                    android:src="@drawable/yq"/>

                <TextView
                    android:id="@+id/web"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="50dip"
                    android:gravity="center_vertical"
                    android:text="@string/w1"
                    android:textColor="#DC413018"
                    android:textSize="15sp" />

                <ImageView
                    android:layout_width="20dip"
                    android:layout_height="20dip"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="5dip"
                    android:background="@drawable/back1" />
            </RelativeLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="2dip"
                android:background="@drawable/line" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="2dip"
                android:background="@drawable/line" />
            <RelativeLayout
                android:id="@+id/ver"
                android:layout_width="match_parent"
                android:layout_height="40dip"
                android:orientation="horizontal" >

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="10sp"
                    android:layout_marginTop="5dp"
                    android:src="@drawable/bb"/>
                <TextView
                    android:id="@+id/version"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="50dip"
                    android:gravity="center_vertical"
                    android:text="@string/a4"
                    android:textColor="#DC413018"
                    android:textSize="15sp" />

                <TextView
                    android:layout_width="20dip"
                    android:layout_height="20dip"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="5dip"
                    android:text="@string/num" />
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>

</FrameLayout>