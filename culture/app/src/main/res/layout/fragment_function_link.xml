<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">
        <ImageButton
            android:id="@+id/t1"
            android:layout_weight="1"
            android:scaleType="centerCrop"
            android:layout_width="130dp"
            android:layout_height="190dp"
            android:src="@drawable/tyg0"
            >
        </ImageButton>
        <ImageButton
            android:id="@+id/t2"
            android:src="@drawable/tyg1"
            android:layout_weight="1"
            android:scaleType="centerCrop"
            android:layout_width="130dp"
            android:layout_height="190dp"
            >
        </ImageButton>
        <ImageButton
            android:id="@+id/t3"
            android:src="@drawable/tyg2"
            android:layout_weight="1"
            android:scaleType="centerCrop"
            android:layout_width="130dp"
            android:layout_height="190dp">
        </ImageButton>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/t4"
            android:src="@drawable/tyg3"
            android:layout_weight="1"
            android:scaleType="centerCrop"
            android:layout_width="130dp"
            android:layout_height="190dp"
            >
        </ImageButton>
        <ImageButton
            android:id="@+id/t5"
            android:src="@drawable/tyg4"
            android:layout_weight="1"
            android:scaleType="centerCrop"
            android:layout_width="130dp"
            android:layout_height="190dp"
            >
        </ImageButton>
        <ImageButton
            android:id="@+id/t6"
            android:src="@drawable/tyg5"
            android:layout_weight="1"
            android:scaleType="centerCrop"
            android:layout_width="130dp"
            android:layout_height="190dp"
            >
        </ImageButton>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">
        <ImageButton
            android:id="@+id/t7"
            android:src="@drawable/tyg6"
            android:layout_weight="1"
            android:scaleType="centerCrop"
            android:layout_width="130dp"
            android:layout_height="190dp"
            >
        </ImageButton>
        <ImageButton
            android:id="@+id/t8"
            android:src="@drawable/tyg7"
            android:scaleType="centerCrop"
            android:layout_weight="1"
            android:layout_width="130dp"
            android:layout_height="190dp"
            >
        </ImageButton>
        <ImageButton
            android:id="@+id/t9"
            android:src="@drawable/tyg8"
            android:scaleType="centerCrop"
            android:layout_weight="1"
            android:layout_width="130dp"
            android:layout_height="190dp"
            >
        </ImageButton>
    </LinearLayout>
</LinearLayout>